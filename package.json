{
  "name": "nodebot-server",
  "description": "Simple socket.io server for controlling a nodebot.",
  "version": "0.1.1",
  "homepage": "https://github.com/doowb/nodebot-server",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/nodebot-server",
  "bugs": {
    "url": "https://github.com/doowb/nodebot-server/issues"
  },
  "license": "MIT",
  "files": [
    "server.js",
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "node ./example.js",
    "test": "mocha"
  },
  "keywords": [
    "assemble",
    "codemash",
    "nodebot",
    "nodebots",
    "socket.io"
  ],
  "dependencies": {
    "component-emitter": "^1.2.1",
    "express": "^4.14.0",
    "extend-shallow": "^2.0.1",
    "socket.io": "^1.7.2"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}
